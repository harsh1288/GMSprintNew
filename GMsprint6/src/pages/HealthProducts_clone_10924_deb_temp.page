    <script type="text/javascript">
        $(document).ready(function() {
            $("#parentCheck").click(function() {
                $(".childCheck").prop('checked', this.checked);
                
            });
            $(".childCheck").click(function(){
                if ($('input.childCheck[type=checkbox]:not(:checked)').length){
                    $("#parentCheck").removeAttr('checked');    
                }else{
                    $("#parentCheck").prop('checked',true);
                }
            });
        })
        function ProdDelConfirmation()
        {
            var status = false;
            var inputElem = document.getElementsByClassName("childCheck"); 
                    for(var i=0; i<inputElem.length; i++) 
                    {
                        if(inputElem[i].checked)
                        {
                            status=true; 
                        }
                    }
            if(status)
            {
                var prodDel = confirm("Are you sure ?");
                if(prodDel == true)
                {
                    DeleteProducts();
                }
            }
            else
            {
                alert('Please select at least one record to continue.');
            }
        }
        function MassEditWhenSelected()
        {
            var sel= false;
            var inputElem = document.getElementsByClassName("childCheck"); 
                    for(var i=0; i<inputElem.length; i++) 
                    {
                        if(inputElem[i].checked)
                        {
                            sel=true; 
                        }
                    }
            if(sel)
            {
                EditAllProducts();
            }
            else
            {
                alert('Please select at least one record to continue.');
            }                
        }
        function checkAll(cb) 
            { 
                var inputElem = document.getElementsByClassName("childCheck"); 
                    for(var i=0; i<inputElem.length; i++) 
                    {
                        inputElem[i].checked = cb; 
                    } 
            }
        function checkParent(val)
            {
                var allchecked = true;
                if(val)
                {
                    var inputElem = document.getElementsByClassName("childCheck"); 
                    for(var i=0; i<inputElem.length; i++) 
                    {
                        if(!inputElem[i].checked)
                        {
                            allchecked = false;    
                        } 
                    }
                    document.getElementById("parentCheck").checked = allchecked;                    
                }
                else
                {
                    document.getElementById("parentCheck").checked = false;
                }
            }
    </script>
    
